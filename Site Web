<!-- #BeginEditable "ventanita" -->

<script type="text/javascript">
/*ventanaemergente*/
$(document).ready(function(){
	$('body #tabla').on('click', 'a', function(){
		var objeto='#'+$(this).attr('id');
		var nal=objeto.substr(4,9);
		var direpcion=$(this).attr('href')+'?c='+nal;
    	$('#fondoatras').addClass('visible').removeClass('novisible');
    	$('#caja').addClass('visible').removeClass('novisible');
		var $iframe = $('#ventanita');
		if ( $iframe.length ) {
			$iframe.attr('src',direpcion);   
			return false;
		}
		return true;
	}) 			
})
</script>	

	<!-- #EndEditable -->
<script>
function espera(){$('#fondoatras').addClass('visible').removeClass('novisible');}
</script>
<script>
$(document).ready(function(){
	$('body ').on('click', '#cajacerrar', function(){
    	$('#ventanita').attr('src','/includes/engranes.html');
    	$('#fondoatras').removeClass('visible').addClass('novisible');
    	$('#caja').removeClass('visible').addClass('novisible');
    	$('#busqueda').focus();
	})
})
$(document).ready(function(){
	$('body ').on('click', '#fondoatras', function(){
    	$('#ventanita').attr('src','/includes/engranes.html');
    	$('#fondoatras').removeClass('visible').addClass('novisible');
    	$('#caja').removeClass('visible').addClass('novisible');
    	$('#busqueda').focus();
	})
})
$(document).keyup(function(event){
    if(event.which==27)
    {
    	$('#ventanita').attr('src','/includes/engranes.html');
    	$('#fondoatras').removeClass('visible').addClass('novisible');
    	$('#caja').removeClass('visible').addClass('novisible');
    	$('#busqueda').focus();
    }
});
var cierra = function(){
    	$('#ventanita').attr('src','/includes/engranes.html');
    	$('#fondoatras').removeClass('visible').addClass('novisible');
    	$('#caja').removeClass('visible').addClass('novisible');
    	$('#busqueda').focus();
}

</script>
