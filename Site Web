<script>
$(document).ready(function(){
	var alto_footer	=$("footer").css("height");
	$("#pie").css('height',alto_footer);
	$(window).resize(function(){
		alto_footer	=$("footer").css("height");
		$("#pie").css('height',alto_footer);
	})
})
</script>
